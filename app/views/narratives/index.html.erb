<h1>Narratives#index</h1>
<p>Find me in app/views/narratives/index.html.erb</p>

<% @narratives.each do |narrative| %>
<div class="row">
      <div class="col-md-4 col-sm-6 portfolio-item">
              <div class="portfolio-hover">
                  <div class="portfolio-hover-content">
                      <i class="fa fa-plus fa-3x"></i>
                      <%= image_tag(narrative.image_url, :class => "img-responsive") %>
                  </div>
              </div>
          <div class="portfolio-caption">

            <tr>
              <td class="col-md-2"> </td>
              <h4>  <%= narrative.title %> </h4>
              <h4>  <%= narrative.description %> </h4>
              <h4>  <%= narrative.result %> </h4>
          </div>
    </tr>
    </div>
</div>
<% end %>
